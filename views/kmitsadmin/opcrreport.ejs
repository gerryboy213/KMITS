                            <td dmx-text="qreport_id" class="text-justify"></td>
                            <td dmx-text="qdocument_code" class="text-justify"></td>
                            <td dmx-text="qrevision_no" class="text-justify"></td>
                            <td dmx-text="qeffectivity" class="text-justify"></td>
