                            <th class="align-middle text-center">Rating</th>
                            <th class="align-middle text-center">Remarks</th>
                            <th class="text-center align-middle d-none">User</th>
                        </tr>
                    </thead>
                    <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="conn_report.data.query" id="tableRepeat2">
                        <tr>
                            <td>

                                <button id="btn_editreport" class="btn btn-success mb-2" dmx-on:click="conn_singlerec.load({qreport_id: qreport_id});modal_edit.show()"><i class="far fa-edit">Edit&nbsp;</i></button><button id="btn_deletereport" class="btn btn-danger" dmx-on:click="conn_deleterec.load({qreport_id: qreport_id});conn_report.load({})">Delete<br></button>
