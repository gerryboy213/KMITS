<dmx-serverconnect id="conn_deleterec" url="/api/admin/data/delete" noload="true"></dmx-serverconnect>
<dmx-serverconnect id="conn_addrec" url="/api/admin/data/create" noload="true"></dmx-serverconnect>
<dmx-serverconnect id="conn_singlerec" url="/api/admin/data/read" noload="true" dmx-param:qreport_id="conn_singlerec.data.query.qreport_id"></dmx-serverconnect>
<dmx-serverconnect id="conn_report" url="/api/admin/data/search" dmx-param:filter="text1.value"></dmx-serverconnect>
<div class="container-fluid">
    <div class="row">
        <div class="col">
            <button id="btn_addreport" class="btn btn-success" dmx-on:click="conn_addrec.load({});modal_add.show()">Add Report</button>

        </div>
        <div class="col">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover table-sm">
                    <thead>
                        <tr>
                            <td></td>
                            <th>Qreport</th>
                            <th>Qdocument code</th>
                            <th>Qrevision no</th>
                            <th>Qeffectivity</th>
                            <th>Qquarter</th>
                            <th>Qname office</th>
                            <th>Qstrategic goals</th>
                            <th>Qsuccess indicatortarget</th>
                            <th>Qalloted budget</th>
                            <th>Qdivision accountable</th>
                            <th>Qactual accomplishment</th>
                            <th>Qaccomplish rate</th>
                            <th>Qrating</th>
                            <th>Qremarks</th>
                            <th>User</th>
                        </tr>
                    </thead>
                    <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="conn_report.data.query" id="tableRepeat2">
                        <tr>
                            <td>

                                <button id="btn_editreport" class="btn btn-success" dmx-on:click="conn_singlerec.load({qreport_id: qreport_id});modal_edit.show()">Edit</button><button id="btn_deletereport" class="btn btn-danger" dmx-on:click="conn_deleterec.load({qreport_id: qreport_id});conn_report.load({})">Delete</button>
                            </td>
                            <td dmx-text="qreport_id"></td>
                            <td dmx-text="qdocument_code"></td>
                            <td dmx-text="qrevision_no"></td>
                            <td dmx-text="qeffectivity"></td>
                            <td dmx-text="qquarter"></td>
                            <td dmx-text="qname_office"></td>
                            <td dmx-text="qstrategic_goals"></td>
                            <td dmx-text="qsuccess_indicatortarget"></td>
                            <td dmx-text="qalloted_budget"></td>
                            <td dmx-text="qdivision_accountable"></td>
                            <td dmx-text="qactual_accomplishment"></td>
                            <td dmx-text="qaccomplish_rate"></td>
                            <td dmx-text="qrating"></td>
                            <td dmx-text="qremarks"></td>
                            <td dmx-text="user_id"></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
